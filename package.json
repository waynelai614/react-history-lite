{
  "name": "react-history-lite",
  "version": "0.1.0",
  "description": "A simple history wrapper for react.js",
  "main": "src/index.js",
  "scripts": {
    "build:production": "webpack --config ./webpack.prod.config.js",
    "dev": "webpack-dev-server --devtool eval --progress --colors --content-base dist",
    "test": "jest test",
    "test:watch": "jest test --watch",
    "eslint": "eslint ."
  },
  "keywords": [
    "react",
    "history"
  ],
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "./test/enzyme_adapter.js"
  },
  "author": "WayneLai",
  "license": "MIT",
  "dependencies": {
    "history": "^4.9.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "recompose": "^0.30.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^23.2.0",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "enzyme-to-json": "^3.1.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-import-resolver-webpack": "^0.10.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.0",
    "eslint-plugin-react": "^7.10.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.3.0",
    "node-sass": "^4.9.2",
    "react-hot-loader": "^4.3.3",
    "react-test-renderer": "^16.0.0",
    "sass-loader": "^7.0.3",
    "sinon": "^7.3.2",
    "style-loader": "^0.21.0",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.15.1",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.4"
